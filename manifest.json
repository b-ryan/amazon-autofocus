{
    "manifest_version": 2,
    "name": "Amazon Search Auto-Focus",
    "description": "Automatically focus on search bar when visiting Amazon",
    "version": "2",
    "permissions": [
        "http://*.amazon.com/",
        "https://*.amazon.com/"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"]
        }
    ]
}
