{
     "manifest_version": 2,
     "name": "Amazon Search Auto-Focus",
     "description": "Automatically focus on search bar when visiting Amazon",
     "version": "1",
     "permissions": [
          "http://*.amazon.com/",
          "https://*.amazon.com/"
     ],
     "content_scripts": [
        {
            "matches": ["http://*.amazon.com/"],
            "js": ["script.js"]
        }
     ]
}
